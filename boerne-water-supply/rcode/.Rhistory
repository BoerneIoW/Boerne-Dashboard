all_data <- read_excel("~/Documents/Dissertation Data/rdata/six_tributary_data.xlsx", sheet = 7, range = "A1:M70")
# Use pivot_longer to reshape the data so months are no longer columns but consecutive values
total_data <- all_data %>% pivot_longer(cols = 2:13, names_to = "Month", values_to = "total_volume")
#IBWC graphic of cycles begins in Oct. 1992, so have the same start point for the dataframe
total_data <- subset(total_data, Year >= 1992) #removes years < 1992
total_data <- total_data[-(1:9),] #remove Jan.-Sept.
# Parse out the portion assigned to the US from Total Volume (1/3)
us_volume <- total_data
us_volume$us_assigned <- total_data$total_volume/3
us_volume <- us_volume %>% mutate(us_assigned_mcm = us_assigned / 1000) # change from thousands of cubic meters to MCM
# Calculate the yearly sums (in this case, a year runs from October to September)
yearly_sums <- us_volume %>%
group_by(Year_int = as.integer(gl(n(), 12, n()))) %>% # this creates a new column, assigning a number to months (i.e., rows) that will be summed together (e.g., 10/1953-09/1954 is Y_int 1)
summarise(total_volume = sum(us_assigned_mcm)) # R will sum according to these groups (i.e., rows with the same Y_int #)
# Cumulative sum every five years (i.e., sum 1-5, reset at 6 and sum 6-10, etc)
cycle_sums <- yearly_sums # these are the yearly observed values, use them to compare the cumulative sums below
cycle_sums$cycles_cumsum <- ave(cycle_sums$total_volume, (seq_along(cycle_sums$total_volume)-1)%/%5 , FUN=cumsum )
# Make a new column for every cycle
cycle_sumsb <- cycle_sums[,-(2)] # drop total_volume and keep cycles_cumsum
cycle_sumsb <- rename(cycle_sumsb, cycle1 = cycles_cumsum) # rename Year_int to cycle1
cycle_sumsb <- cycle_sumsb %>% # Create duplicates of cycle1 (there are 30 yrs in the dataframe, so should be 6 cycles)
mutate(cycle2 = cycle1) %>%
mutate(cycle3 = cycle1) %>%
mutate(cycle4 = cycle1) %>%
mutate(cycle5 = cycle1) %>%
mutate(cycle6 = cycle1) %>%
mutate(target = cycle1)
# Remove values in each cycle so that there are only 5 yrs of values in each
cycle_sumsc <- cycle_sumsb
cycle_sumsc$cycle1 <- replace(cycle_sumsc$cycle1, 6:30, "")
cycle_sumsc$cycle2 <- replace(cycle_sumsc$cycle2, c(1:5,11:30), ""); cycle_sumsc$cycle2 <- replace(cycle_sumsc$cycle2, 5, 0)
cycle_sumsc$cycle3 <- replace(cycle_sumsc$cycle3, c(1:10,16:30), ""); cycle_sumsc$cycle3 <- replace(cycle_sumsc$cycle3, 10, 0)
cycle_sumsc$cycle4 <- replace(cycle_sumsc$cycle4, c(1:15,21:30), ""); cycle_sumsc$cycle4 <- replace(cycle_sumsc$cycle4, 15, 0)
cycle_sumsc$cycle5 <- replace(cycle_sumsc$cycle5, c(1:20,26:30), ""); cycle_sumsc$cycle5 <- replace(cycle_sumsc$cycle5, 20, 0);
cycle_sumsc$cycle6 <- replace(cycle_sumsc$cycle6, 1:25, ""); cycle_sumsc$cycle6 <- replace(cycle_sumsc$cycle6, 25, 0)
cycle_sumsc$target <- replace(cycle_sumsc$target, 1:31, "2158.6") # this is for a horizontal ongoing target (instead of cycle by cycle)
cycle_sumsc$target <- replace(cycle_sumsc$target, 1:30, "2158.6") # this is for a horizontal ongoing target (instead of cycle by cycle)
# Add year zero so cycle 1 can start with a delivery value of 0 instead of the value @ year 1
cycle_sumsd <- rbind(c(0,0,"","","","","","2158.6"), cycle_sumsc)
